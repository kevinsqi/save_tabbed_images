    "pluralize": "~1.2.0",
    "react": "~15.0.0",
    "react-addons-update": "^15.0.1",
    "react-dom": "~15.0.0",
    "underscore": "~1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-plugin-react": "^3.11.3",
    "webpack": "^1.12.9"
  }
}
